        Users and Logins
      </div>
      <div class='scrollable'>
        scrollable
      </div>
    </div>
    <div class='content'>
      content
      <div class='header'>
        Games and Status
      </div>
      <div class='alerts'>
        alerts
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
      </div>
      <div class='scrollable'>
        scrollable
      </div>
    </div>
    <div class='sidebar_right'>
      sidebar
      <div class='header'>
        header
      </div>
      <div class='scrollable'>
        scrollable
      </div>
    </div>
    <div class='footer'>
      footer
    </div>
