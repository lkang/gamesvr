<div class='container'>
  <div class='header'>
    <div class='logo'>
    </div>
    <div class='loggedin'>
      <%if @user%>
        <p>Hello, <%=@user.email%> | <%=link_to('logout', destroy_user_session_path, :method=>:delete)%></p>
      <%elsif @session_user %>
        <p>Hello, <%=@session_user%> | <%=link_to('logout', fb_login_path, :method=>:delete)%></p>
      <% else %>
      <!-- <p><%=link_to('login', new_user_session_url)%> | <%= link_to 'facebook login', new_fb_login_url %></p> -->
      <p><%=link_to('login', new_user_session_url)%> | <%= link_to "Login with Facebook", fb_login_url, :method => :post %></p>
      <%end%>
    </div>
    <h2> This is the homepage! </h2>
  </div>
  <div class='main_content'>
    <div class='sidebar_left'>
      sidebar
